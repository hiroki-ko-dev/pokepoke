{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".", 
      "paths": {
        "@/*": ["resources/js/*"]
      },
    },
    "include": [
      "global.d.ts",
      "resources/js/**/*",
      "resources/ts/**/*",
    ]
  }
  